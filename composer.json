{
    "name": "flyingwhale/optionable",
    "type": "library",
    "description": "Optionable is an extension to Pimple Dependency Injection Container. It handles default values.",
    "keywords": ["dependency injection", "container", "default value"],
    "license": "Apache-2.0",
    "authors": [
        {
          "name":   "Zsolt Javorszky",
          "email":  "zsolt.javorszky@gmail.com"
        },
        {
          "name":   "Imre Toth",
          "email":  "tothimre@gmail.com"
        }
    ],
    "require": {
        "php":                  ">=5.3.0",
        "pimple/pimple":        "1.*"
    },
    "require-dev": {
	"phpunit/phpunit": "4.1.0"
    },
    "autoload": {
        "psr-0": { "Optionable": "src/" }
    },
    "process-timeout": "3600"
}
